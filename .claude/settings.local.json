{
  "permissions": {
    "allow": [
      "Bash(mvn clean install:*)",
      "Bash(mvn spring-boot:run:*)",
      "Bash(docker port:*)",
      "Bash(docker exec:*)",
      "Bash(docker inspect:*)",
      "Bash(docker logs:*)",
      "Bash(docker network:*)",
      "Bash(docker stop:*)",
      "Bash(docker rm:*)",
      "Bash(docker run:*)",
      "Bash(timeout 15 bash:*)",
      "Bash(for code in SPK RFM TRM ENV WM PAT PRP STR WEM GDD MTU AGD AEP)",
      "Bash(do docker exec kafka-claims /opt/kafka/bin/kafka-topics.sh --create --bootstrap-server localhost:9092 --topic claims.$code --partitions 3 --replication-factor 1 --if-not-exists)",
      "Bash(done)",
      "Bash(mvn clean compile:*)",
      "Bash(netstat:*)",
      "Bash(findstr:*)",
      "Bash(taskkill:*)",
      "Bash(cmd //c taskkill //PID 29300 //F)",
      "Bash(cmd //c taskkill //PID 29932 //F)",
      "Bash(cmd //c \"docker exec kafka-claims //opt//kafka//bin//kafka-topics.sh --list --bootstrap-server localhost:9092\")",
      "Bash(cmd //c:*)",
      "Bash(ls:*)",
      "Bash(jps:*)",
      "Bash(kafka-console-consumer:*)",
      "Bash(where:*)",
      "Bash(mysql:*)",
      "Bash(docker ps:*)",
      "Bash(cmd.exe /c \"docker exec kafka-claims ls /bin\")",
      "Bash(docker restart:*)",
      "Bash(timeout 20 bash -c 'until docker exec kafka-claims test -f /tmp/kraft-combined-logs/meta.properties; do echo \"Waiting for Kafka...\"; sleep 2; done && echo \"Kafka is ready!\"':*)"
    ]
  }
}
